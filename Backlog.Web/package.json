{
  "name": "backlog-app",
  "version": "1.0.0",
  "description": "backlog-app",
  "main": "index.html",
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "webdriver-manager": "webdriver-manager",
    "protractor": "protractor",
    "watch": "webpack --watch",
    "docs": "npm run typedoc -- --options typedoc.json --includeDeclarations typings/index.d.ts --exclude '**/*.spec.ts' ./src/",
    "minify": "npm run minify-vendor && npm run minify-schedule && npm run minify-polyfills",
    "minify-schedule": "uglifyjs dist/schedule.js --screw-ie8 --compress --mangle --output dist/schedule.min.js",
    "minify-vendor": "uglifyjs dist/vendor.js --screw-ie8 --compress --mangle --output dist/vendor.min.js",
    "minify-polyfills": "uglifyjs dist/polyfills.js --screw-ie8 --compress --mangle --output dist/polyfills.min.js"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@ngrx/core": "^1.0.1",
    "@ngrx/store": "^2.0.1",
    "ngrx-store-localstorage": "0.1.4",
    "core-js": "2.4.1",
    "css-loader": "0.23.1",
    "es6-shim": "0.35.1",
    "html-loader": "0.4.3",
    "ie-shim": "^0.1.0",
    "istanbul": "^0.4.4",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "jquery": "^3.1.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-sourcemap-writer": "^0.1.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "node-sass": "3.8.0",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "sass-loader": "4.0.0",
    "slick-carousel": "^1.6.0",
    "style-loader": "0.13.1",
    "ts-loader": "0.8.2",
    "typescript": "1.8.10",
    "typings": "1.3.2",
    "webpack": "1.13.1",
    "zone.js": "0.6.23"
  },
